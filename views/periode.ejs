<h3>Data Periode</h3>

<form action="/addPeriode" method="post">
  <div>
    <label for="periode">Periode:</label>
    <input type="text" id="periode" name="periode" required>
  </div>
  <div>
    <label for="nilai">Nilai:</label>
    <input type="number" id="nilai" name="nilai" required>
  </div>
  <div>
    <button type="submit">Submit</button>
  </div>
</form>

<div>
    <% let no = 1; %>
    <% data.forEach(e => { %>
    <div>
      <p><%= no %> || <%= e.periode %> | <%= e.nilai %> || <a href="/editPeriode/<%= e.id %>">Edit</a> |  <a href="/deletePeriode/<%= e.id %>">Delete</a></p>
    </div>
    <% no++ %>
    <% }) %>
</div>